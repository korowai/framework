{
    "name": "korowai/framework",
    "require": {
        "php": ">=7.4",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "league/uri": "^6.3",
        "league/uri-interfaces": "^2.1",
        "symfony/options-resolver": ">=4.2"
    },
    "autoload": {
        "exclude-from-classmap": [
            "packages/basiclib-interfaces/testing/",
            "packages/basiclib-interfaces/tests/",
            "packages/basiclib/testing/",
            "packages/basiclib/tests/",
            "packages/compatlib/testing/",
            "packages/compatlib/tests/",
            "packages/contextlib-interfaces/testing/",
            "packages/contextlib-interfaces/tests/",
            "packages/contextlib/testing/",
            "packages/contextlib/tests/",
            "packages/errorlib-interfaces/testing/",
            "packages/errorlib-interfaces/tests/",
            "packages/errorlib/testing/",
            "packages/errorlib/tests/",
            "packages/ldaplib-interfaces/testing/",
            "packages/ldaplib-interfaces/tests/",
            "packages/ldaplib/behat/",
            "packages/ldaplib/config/",
            "packages/ldaplib/resources/",
            "packages/ldaplib/testing/",
            "packages/ldaplib/tests/",
            "packages/ldiflib-interfaces/testing/",
            "packages/ldiflib-interfaces/tests/",
            "packages/ldiflib/testing/",
            "packages/ldiflib/tests/",
            "packages/rfclib-interfaces/testing/",
            "packages/rfclib-interfaces/tests/",
            "packages/rfclib/testing/",
            "packages/rfclib/tests-nocov/",
            "packages/rfclib/tests/",
            "packages/testing/testing/",
            "packages/testing/tests/"
        ],
        "files": [
            "packages/compatlib/src/functions/preg.php",
            "packages/contextlib/src/functions.php",
            "packages/errorlib/src/functions.php",
            "packages/ldaplib/config/functions.php",
            "packages/testing/config/functions.php"
        ],
        "psr-4": {
            "Korowai\\Lib\\Basic\\": [
                "packages/basiclib-interfaces/src/",
                "packages/basiclib/src/"
            ],
            "Korowai\\Lib\\Compat\\": "packages/compatlib/src/",
            "Korowai\\Lib\\Context\\": [
                "packages/contextlib-interfaces/src/",
                "packages/contextlib/src/"
            ],
            "Korowai\\Lib\\Error\\": [
                "packages/errorlib-interfaces/src/",
                "packages/errorlib/src/"
            ],
            "Korowai\\Lib\\Ldap\\": [
                "packages/ldaplib-interfaces/src/",
                "packages/ldaplib/src/"
            ],
            "Korowai\\Lib\\Ldif\\": [
                "packages/ldiflib-interfaces/src/",
                "packages/ldiflib/src/"
            ],
            "Korowai\\Lib\\Rfc\\": [
                "packages/rfclib-interfaces/src/",
                "packages/rfclib/src/"
            ],
            "Korowai\\Testing\\": "packages/testing/testing/",
            "Korowai\\Testing\\BasiclibInterfaces\\": "packages/basiclib-interfaces/testing/",
            "Korowai\\Testing\\Basiclib\\": "packages/basiclib/testing/",
            "Korowai\\Testing\\Compatlib\\": "packages/compatlib/testing/",
            "Korowai\\Testing\\ContextlibInterfaces\\": "packages/contextlib-interfaces/testing/",
            "Korowai\\Testing\\Contextlib\\": "packages/contextlib/testing/",
            "Korowai\\Testing\\ErrorlibInterfaces\\": "packages/errorlib-interfaces/testing/",
            "Korowai\\Testing\\Errorlib\\": "packages/errorlib/testing/",
            "Korowai\\Testing\\LdaplibInterfaces\\": "packages/ldaplib-interfaces/testing/",
            "Korowai\\Testing\\Ldaplib\\": "packages/ldaplib/testing/",
            "Korowai\\Testing\\LdiflibInterfaces\\": "packages/ldiflib-interfaces/testing/",
            "Korowai\\Testing\\Ldiflib\\": "packages/ldiflib/testing/",
            "Korowai\\Testing\\RfclibInterfaces\\": "packages/rfclib-interfaces/testing/",
            "Korowai\\Testing\\Rfclib\\": "packages/rfclib/testing/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Korowai\\Docs\\Behat\\": [
                "docs/sphinx/behat/"
            ],
            "Korowai\\Lib\\Ldap\\Behat\\": "packages/ldaplib/behat/",
            "Korowai\\TestsNocov\\Lib\\Rfc\\": "packages/rfclib/tests-nocov/",
            "Korowai\\Tests\\Ldaplib\\": "packages/ldaplib/tests/ldaplib/",
            "Korowai\\Tests\\Lib\\Basic\\": [
                "packages/basiclib-interfaces/tests/src/",
                "packages/basiclib/tests/src/"
            ],
            "Korowai\\Tests\\Lib\\Compat\\": "packages/compatlib/tests/src/",
            "Korowai\\Tests\\Lib\\Context\\": [
                "packages/contextlib-interfaces/tests/src/",
                "packages/contextlib/tests/src/"
            ],
            "Korowai\\Tests\\Lib\\Error\\": [
                "packages/errorlib-interfaces/tests/src/",
                "packages/errorlib/tests/src/"
            ],
            "Korowai\\Tests\\Lib\\Ldap\\": [
                "packages/ldaplib-interfaces/tests/src/",
                "packages/ldaplib/tests/src/"
            ],
            "Korowai\\Tests\\Lib\\Ldif\\": [
                "packages/ldiflib-interfaces/tests/src/",
                "packages/ldiflib/tests/src/"
            ],
            "Korowai\\Tests\\Lib\\Rfc\\": [
                "packages/rfclib-interfaces/tests/src/",
                "packages/rfclib/tests/src/"
            ],
            "Korowai\\Tests\\Testing\\": "packages/testing/tests/testing/",
            "Korowai\\Tests\\Testing\\BasiclibInterfaces\\": "packages/basiclib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Basiclib\\": "packages/basiclib/tests/testing/",
            "Korowai\\Tests\\Testing\\Compatlib\\": "packages/compatlib/tests/testing/",
            "Korowai\\Tests\\Testing\\ContextlibInterfaces\\": "packages/contextlib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Contextlib\\": "packages/contextlib/tests/testing/",
            "Korowai\\Tests\\Testing\\ErrorlibInterfaces\\": "packages/errorlib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Errorlib\\": "packages/errorlib/tests/testing/",
            "Korowai\\Tests\\Testing\\LdaplibInterfaces\\": "packages/ldaplib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Ldaplib\\": "packages/ldaplib/tests/testing/",
            "Korowai\\Tests\\Testing\\LdiflibInterfaces\\": "packages/ldiflib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Ldiflib\\": "packages/ldiflib/tests/testing/",
            "Korowai\\Tests\\Testing\\RfclibInterfaces\\": "packages/rfclib-interfaces/tests/testing/",
            "Korowai\\Tests\\Testing\\Rfclib\\": "packages/rfclib/tests/testing/"
        }
    },
    "replace": {
        "korowai/basiclib": "self.version",
        "korowai/basiclib-interfaces": "self.version",
        "korowai/compatlib": "self.version",
        "korowai/contextlib": "self.version",
        "korowai/contextlib-interfaces": "self.version",
        "korowai/contracts": "self.version",
        "korowai/errorlib": "self.version",
        "korowai/errorlib-interfaces": "self.version",
        "korowai/ldaplib": "self.version",
        "korowai/ldaplib-interfaces": "self.version",
        "korowai/ldiflib": "self.version",
        "korowai/ldiflib-interfaces": "self.version",
        "korowai/rfclib": "self.version",
        "korowai/rfclib-interfaces": "self.version",
        "korowai/testing": "self.version"
    },
    "description": "Framework for Korowai project",
    "license": "MIT",
    "require-dev": {
        "behat/behat": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "illuminate/container": ">=8.5.0",
        "phake/phake": "^3.0",
        "php-di/php-di": ">=6.2.2",
        "php-mock/php-mock-phpunit": ">=2.4.0",
        "phptailors/phpunit-inheritance": "dev-master",
        "phptailors/phpunit-properties": "dev-master",
        "phptailors/phpunit-regexp": "dev-master",
        "phpunit/phpunit": ">=9.3",
        "psr/container": "1.0",
        "psy/psysh": "dev-main",
        "symfony/config": ">=5.1.5",
        "symfony/dependency-injection": ">=5.1.5",
        "symplify/monorepo-builder": "^8.0.1",
        "zendframework/zend-ldap": "^2.10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
