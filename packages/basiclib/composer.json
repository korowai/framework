{
    "name": "korowai/basiclib",
    "description": "Basic general-purpose code used across korowai-framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3",
        "korowai/testing": "*"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Basic\\" : "src/",
        "Korowai\\Testing\\Basiclib\\" : "Testing/"
      },
      "exclude-from-classmap": [
        "/tests/",
        "/Testing/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Tests\\Lib\\Basic\\" : "tests/"
      }
    }
}
