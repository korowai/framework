{
    "name": "korowai/compatlib",
    "description": "A compatibility layer between korowai and PHP libraries",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "require": {
        "php": ">=7.4",
        "korowai/contextlib": "*",
        "korowai/errorlib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3",
        "korowai/testing": "*"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Compat\\" : "src/",
        "Korowai\\Testing\\Compatlib\\" : "testing/"
      },
      "files" : [
        "src/functions/preg.php"
      ],
      "exclude-from-classmap": [
        "/tests/",
        "/testing/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Tests\\Lib\\Compat\\" : "tests/src/",
        "Korowai\\Tests\\Testing\\Compatlib\\" : "tests/testing/"
      }
    }
}
