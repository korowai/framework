{
    "name": "korowai/ldaplib",
    "description": "Korowai LDAP component",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "require": {
        "php": ">=7.4",
        "ext-ldap": "*",
        "symfony/options-resolver": ">=4.2",
        "korowai/basiclib": "*",
        "korowai/compatlib": "*",
        "korowai/contextlib": "*",
        "korowai/errorlib": "*",
        "korowai/ldaplib-interfaces": "*",
        "korowai/rfclib": "*"
    },
    "provide": {
        "korowai/ldaplib-implementation": "*"
    },
    "require-dev": {
        "zendframework/zend-ldap": "^2.10.0",
        "phpunit/phpunit": ">=9.3",
        "php-mock/php-mock-phpunit": ">=2.4.0",
        "php-di/php-di": ">=6.2.2",
        "psr/container": "1.0",
        "korowai/testing": "*"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Ldap\\" : "src/",
        "Korowai\\Testing\\Ldaplib\\" : "Testing/"
      },
      "exclude-from-classmap": [
        "/Behat/",
        "/Resources/",
        "/Testing/",
        "/tests/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Lib\\Ldap\\Behat\\" : "Behat/",
        "Korowai\\Tests\\Lib\\Ldap\\" : "tests/",
        "Korowai\\Tests\\Testing\\Ldaplib\\" : "tests/Testing/"
      }
    }
}
