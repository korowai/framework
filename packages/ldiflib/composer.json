{
    "name": "korowai/ldiflib",
    "description": "A library for handling LDIF text",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suport": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "korowai/contracts": "*",
        "korowai/contextlib": "*",
        "korowai/errorlib": "*"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Ldif\\" : "src/Korowai/Lib/Ldif/"
      },
      "files" : [
        "src/Korowai/Lib/Ldif/Util/preprocessing.php"
      ],
      "exclude-from-classmap": [
        "/Tests/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Lib\\Ldif\\Tests\\" : "Tests/"
      }
    }
}
