{
    "name": "korowai/ldiflib",
    "description": "A library for handling LDIF text",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "korowai/compatlib": "*",
        "korowai/contextlib": "*",
        "korowai/errorlib": "*",
        "korowai/ldiflib-interfaces": "*",
        "korowai/rfclib": "*",
        "symfony/options-resolver": ">=4.2",
        "league/uri": "^6.3",
        "league/uri-interfaces": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3",
        "korowai/testing": "*",
        "phptailors/phpunit-inheritance": "dev-master",
        "phptailors/phpunit-properties": "dev-master",
        "phptailors/phpunit-regexp": "dev-master"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Ldif\\" : "src/",
        "Korowai\\Testing\\Ldiflib\\" : "testing/"
      },
      "files" : [
      ],
      "exclude-from-classmap": [
        "/tests/",
        "/testing/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Tests\\Lib\\Ldif\\" : "tests/src/",
        "Korowai\\Tests\\Testing\\Ldiflib\\" : "tests/testing/"
      }
    }
}
