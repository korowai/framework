{
    "name": "korowai/ldiflib",
    "description": "A library for handling LDIF text",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "korowai/compatlib": "*",
        "korowai/contextlib": "*",
        "korowai/contracts": "*",
        "korowai/errorlib": "*",
        "korowai/rfclib": "*",
        "symfony/options-resolver": ">=4.2",
        "league/uri": "^6.0",
        "league/uri-interfaces": "^2.1"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Lib\\Ldif\\" : "src/"
      },
      "files" : [
      ],
      "exclude-from-classmap": [
        "/tests/",
        "/Testing/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Tests\\Lib\\Ldif\\" : "tests/",
        "Korowai\\Testing\\Ldiflib\\" : "Testing/"
      }
    }
}
