{
    "name": "korowai/testing",
    "description": "A library of reusable classes and traits for unit-testing",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
      "issues": "https://github.com/korowai/framework/issues",
      "source": "https://github.com/korowai/framework"
    },
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "require": {
        "php": ">=7.4",
        "korowai/basiclib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3",
        "psr/container": "1.0",
        "illuminate/container": ">=8.5.0",
        "php-di/php-di": ">=6.2.2",
        "symfony/config": ">=5.1.5",
        "symfony/dependency-injection": ">=5.1.5",
        "phptailors/phpunit-inheritance": "dev-master",
        "phptailors/phpunit-regexp": "dev-master",
        "phptailors/phpunit-properties": "dev-master"
    },
    "autoload": {
      "psr-4" : {
        "Korowai\\Testing\\" : "testing/"
      },
      "files": [
        "config/functions.php"
      ],
      "exclude-from-classmap": [
        "/tests/",
        "/testing/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Korowai\\Tests\\Testing\\" : "tests/testing/"
      }
    }
}
